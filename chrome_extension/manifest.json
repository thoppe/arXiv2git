{
    "manifest_version": 2,
    "name": "arXiv2git",
    "version": "0.01",
    "description": "Looks for github links from arXiv pages. https://github.com/thoppe/arXiv2git",

    "content_scripts": [
        {
            "matches": [
                "http://arxiv.org/*",
                "https://arxiv.org/*"
            ],
            "js": [
                "jquery-2.2.3.min.js",
                "content.js"
            ],
            "css": [
                "style.css"
            ]
        }
    ]
}
